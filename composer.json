{
    "name": "pumahawk/easyfrphp",
    "description": "A minimal php framework",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Pumahawk/RouterRequest.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Pumahawk/Temply.git"
        }
    ],
    "require": {
        "php": "^7.0",
        "pumahawk/routerrequest": "dev-support-easyfrphp",
        "pumahawk/temply": "dev-support-easyfrphp"
    },
    "autoload": {
      "psr-4": {
        "EasyFrPHP\\": "bin/objects/EasyFrPHP/"
      }
    },
    "require-dev": {
      "phpunit/phpunit": "^7",
      "symfony/console": "^4.1"
    }
}
